{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "extends": [
    "airbnb",
    "vue",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "arrow-parens": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-unused-vars": 1,
    "no-undef": 0,
    "indent": 0,
    "no-console": 1,
    "no-new": 2,
    "max-len": 0,
    "no-multiple-empty-lines": [
      2, {
        "max": 3
      }
    ],
    "object-curly-spacing": 1,
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "func-names": 0,
    "space-before-function-paren": 0,
    "import/no-unresolved": [
    2,
    {
      "commonjs": true,
      "amd": true
    }
    ],
    "import/named": 2,
    "import/namespace": 2,
    "import/default": 2,
    "import/export": 2
  }
}
